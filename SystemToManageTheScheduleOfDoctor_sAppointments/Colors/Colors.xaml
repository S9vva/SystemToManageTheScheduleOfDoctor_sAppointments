<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Colors -->

    <!-- Background Colors-->
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#FFFF"/>

    <!-- Primary Colors -->
    <SolidColorBrush x:Key="PrimaryBlueColor" Color="#004BFF"/>
    <SolidColorBrush x:Key="PrimaryGrayColor" Color="#64677A"/>

    <!-- Secundary Colors-->
    <SolidColorBrush x:Key="SecundaryBlueColor" Color="#496FFF"/>
    <SolidColorBrush x:Key="SecundaryGrayColor" Color="#9295A5"/>

    <!-- Tertiary Colors-->
    <SolidColorBrush x:Key="TertiaryBlueColor" Color="#496FFF"/>
    <SolidColorBrush x:Key="TertiaryGrayColor" Color="#CECECE"/>

    <!-- Text Colors-->
    <SolidColorBrush x:Key="PrimaryTextColor" Color="#010101"/>
    

</ResourceDictionary>